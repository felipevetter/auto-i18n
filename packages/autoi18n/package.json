{
    "name": "@scopeact/autoi18n",
    "version": "1.1.3",
    "description": "Auto i18n tool",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "@scopeact/autoi18n": "./dist/cli.js"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./core": {
            "types": "./dist/core/index.d.ts",
            "import": "./dist/core/index.js",
            "require": "./dist/core/index.cjs"
        }
    },
    "files": [
        "dist",
        "schema.json"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "tsc --noEmit"
    },
    "keywords": [
        "i18n",
        "translation",
        "automation",
        "ai"
    ],
    "author": "Felipe Vetter",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "chalk": "^5.6.2",
        "commander": "^14.0.2",
        "dotenv": "^17.2.3",
        "openai": "^6.14.0",
        "ora": "^9.0.0",
        "ts-morph": "^27.0.2"
    },
    "devDependencies": {
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    }
}